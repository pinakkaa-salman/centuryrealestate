            jQuery(input_mobileno).prop('readonly', true);

            setTimeout(function() {
                jQuery(input_mobileno).prop('readonly', false);

            }, 3000);


            if (name == "") {
                //alert("Name");
                return false;
            }



            var atpos = emailid.indexOf("@");
            var dotpos = emailid.lastIndexOf(".");
            if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= emailid.length) {
                return false;
            }

            if (name != "" && mobileno != "" && emailid != "") {

                setTimeout(function() {

                    $("#pageloader").fadeIn();

                    $('#' + elm).submit();

                    if (elm == 'broucher-form') {
                        document.getElementById('broucher1').click();

                    }

                    if (elm == 'floor-form') {
                        document.getElementById('floor1').click();

                    }

                    return true;
                }, 2000);

            } else {
                return false;
            }
        }
    }
